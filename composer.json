{
    "name": "turandemirs/laravel-smsservices",
    "description": "Persistent sms package for Laravel",
    "keywords": [
        "fowitech",
        "turandemirs",
        "laravel",
        "twilio",
        "netgsm",
        "iletimerkezi",
        "asist",
        "sms"
    ],
    "homepage": "https://github.com/turandemirs/laravel-smsservices",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": turan demirs",           
            "role": "Developer"
        }
    ],  
      "require": {
        "php": "^7.4 || ^8.2",
         "laravel/framework": "^8.0 || ^9.0 || ^10.0",
        "turandemirs/laravel-sms": "^1.0"
    },
      "minimum-stability": "dev",
      "prefer-stable": false,
    "require-dev": {
        "phpunit/phpunit": "^7.0 || ^8.2"
    },
    "autoload": {
        "psr-4": {
            "Turandemir\\SmsServices\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Turandemir\\SmsServices\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
     "repositories": [
        {"type": "composer", "url": "https://repo.packagist.com/turandemirs/"},
        {"packagist.org": false}
    ]
   
}
