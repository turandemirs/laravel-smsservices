{
    "name": "turandemirs/laravel-smsservices",
    "description": "Persistent sms package for Laravel",
    "keywords": [
        "fowitech",
        "turandemirs",
        "laravel",
        "twilio",
        "netgsm",
        "iletimerkezi",
        "sms"
    ],
    "homepage": "https://github.com/fowitech/laravel-sms",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": turan demirs",           
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1 || ^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0 || ^8.1"
    },
    "autoload": {
        "psr-4": {
            "Turandemir\\SmsServices\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Turandemir\\SmsServices\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Turandemir\\SmsServices\\SmsServiceProvider"
            ],
            "aliases": {
                "Sms": "Turandemir\\SmsServices\\Facades\\Sms"
            }
        }
    },
    "minimum-stability": "dev"
}
